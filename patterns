import java.util.*;

public class Main {
    public static void main(String[] args) {
      int n=5;
      for(int i=1;i<=n;i++){
        for(int j=1;j<=i-1;j++){
          System.out.print(" ");
        }
        for(int j=5;j>=i;j--){
          System.out.print("*");
        }
      System.out.println();
      }
  }
}

Output:

